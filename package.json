{
  "name": "tx-composer",
  "version": "0.1.0",
  "description": "Simulate-first DeFi transaction composer for Aptos",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "aptos",
    "defi",
    "transaction",
    "composer",
    "script-composer",
    "simulate",
    "move",
    "blockchain"
  ],
  "dependencies": {
    "@aptos-labs/script-composer-pack": "^0.2.1",
    "@aptos-labs/script-composer-sdk": "^0.3.2",
    "@aptos-labs/ts-sdk": "^5.2.1",
    "got": "^14.6.6"
  },
  "peerDependencies": {
    "@aptos-labs/forklift": "^1.0.1"
  },
  "peerDependenciesMeta": {
    "@aptos-labs/forklift": {
      "optional": true
    }
  },
  "devDependencies": {
    "@aptos-labs/forklift": "^1.0.1",
    "@types/node": "^25.0.0",
    "typescript": "^5.9.0"
  },
  "engines": {
    "node": ">=18"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/seammoney/tx-composer"
  }
}
